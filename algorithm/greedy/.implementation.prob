```cpp
#include <iostream>
#include <string>

using namespace std;

int chk[8] = { 0b100100100, 0b010010010, 0b001001001, 0b111000000, 0b000111000, 0b000000111, 0b100010001, 0b001010100 };

bool match(int bit) {
  for (int i = 0; i < 8; ++i)
    if ((bit & chk[i]) == chk[i])
      return true;

  return false;
}

int main() {
  while (true) {
    string str;
    int oBit = 0, xBit = 0;
    int oCnt = 0, xCnt = 0;

    cin >> str;
    if (str == "end")
      break;

    for (char& c : str) {
      if (c == 'O') {
        ++oBit;
        ++oCnt;
      }
      else if (c == 'X') {
        ++xBit;
        ++xCnt;
      }

      oBit <<= 1;
      xBit <<= 1;
    }
    oBit >>= 1;
    xBit >>= 1;

    if (xCnt < oCnt || oCnt + 1 < xCnt || xCnt < 3)
      cout << "invalid\n";
    else if (xCnt > oCnt && match(xBit) && !match(oBit))
      cout << "valid\n";
    else if (xCnt == oCnt && match(oBit) && !match(xBit))
      cout << "valid\n";
    else if (xCnt + oCnt == 9 && !match(xBit) && !match(oBit))
      cout << "valid\n";
    else
      cout << "invalid\n";
  }

  return 0;
}
```

