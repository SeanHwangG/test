[
  {
    "key": "cmd+0",
    "command": "type",
    "args": {
      "text": "```txt\nInput:\nOutput:\n```\n" // Input output format
    },
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+1",
    "command": "type",
    "args": {
      "text": "{% tabs %}\n{% tab title='' %}\n{% endtab %}\n{% endtabs %}\n"
    },
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+8",
    "command": "type",
    "args": {
      "text": "[//]: # ()" // comment in markdown for duplicate question
    },
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+9",
    "command": "type",
    "args": {
      "text": "> Question\n"
    },
    "when": "editorTextFocus"
  }
]
