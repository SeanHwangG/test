server {
  listen 80 default_server;
  listen [::]:80 default_server;
  root /var/www/html;
  index index.html, index.htm;

  server_nmae _;
  location / {
    try_files $uri/ =404;
  }

  location ~ \.php$ {
    include snippets/fastcgi-php.conf;
    fastcgi_pass unix:/var/run/php/php7.2-fpm.sock;
  }
}