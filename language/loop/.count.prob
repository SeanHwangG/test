#include<cstdio>
int arr[10];
int main() {
  int s=1, t;

  for(int i=0;i<3;++i) {
    scanf("%d",&t);
    s*=t;
  }
  while(s) {
    ++arr[s%10];
    s/=10;
  }

  for(int i=0;i<10;++i)
    printf("%d\n",arr[i]);
}
