#include <vector>
#include <iostream>
using namespace std;

int main() {
  vector<int> music;
  int N, L, D;
  int bell = 0;

  cin >> N >> L >> D;

  for (int i = 0; i < N; i++) {
    for (int j = 0; j < L; j++)
      music.push_back(1);
    for (int j = 0; j < 5; j++)
      music.push_back(0);
  }

  while (1) {
    if (bell > music.size() || music[bell] == 0) // bell after song || bell between song
      break;
    else
      bell = bell + D;
  }
  cout << bell;
}
```

