```cpp
#include <bits/stdc++.h>
int main() {
  int s, a, b, c, d, e, f;
  for(; ~scanf("%d:%d:%d %d:%d:%d", &a, &b, &c, &d, &e, &f); ){
    s = 0
    while (true) {
      if((a + b + c) % 3 == 0) s++;
      if(a == d && b == e && c == f) break;
      c++;
      if(c == 60) c = 0, b ++;
      if(b == 60) b = 0, a ++;
      if(a == 24) a = 0;
    }
    printf("%d\n", s);
  }
}
