var s = require('fs').readFileSync('/dev/stdin').toString().trim().split('');
var a = [], c = 0;
while (c <= 1 && s.length > 1) {
  var t = s.shift(), k = s.indexOf(t);
  if(k === -1) {
    ++c
    s.push(t);
  } else {
    a.push(t);
    s.splice(k, 1);
  }
}
